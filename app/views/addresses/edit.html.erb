<div class="container">
  <div class="row">
    <div class="address-edit">
      <div class="title">
        <p>配送先編集</p>
      </div>
      <div class="address-edit-form">
        <%= form_with model: @address, url:address_path, local: true do |f| %>
        <p>郵便番号<%= f.text_field :postal_code %></p>
        <p>住所<%= f.text_field :address %></p>
        <p>宛名<%= f.text_field :name %></p>
        <p><%= f.submit "変更内容を保存", class:"btn btn-sm btn-success" %></p>
        <% end %>
      </div>
    </div>
  </div>
</div>