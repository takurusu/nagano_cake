<div class="container">
  <div class="row">
    <div class="title">
      <p>会員一覧</p>
    </div>
    <table class="table table-stripped">
      <thread>
        <th>会員ID</th>
        <th>氏名</th>
        <th>メールアドレス</th>
        <th>ステータス</th>
      </thread>
      <tbody>
        <% @customers.each do |customer| %>
          <tr>
            <td><%= customer.id %></td>
            <td><%= link_to admin_customer_path(customer) do %>
                  <%= customer.last_name %><%= customer.first_name %>
                <% end %></td>
            <td><%= customer.email %></td>
            <td><%= customer.is_active %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>