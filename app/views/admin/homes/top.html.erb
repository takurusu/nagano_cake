<div class="container">
  <div class="row">
    <div class="title">
      <p>注文履歴一覧</p>
    </div>
    <table class="table table-stripped">
      <thread>
        <th>商品ID</th>
        <th>商品名</th>
        <th>税抜価格</th>
        <th>ジャンル</th>
        <th>ステータス</th>
      </thread>
      <tbody>
        <% @items.each do |item| %>
          <tr>
            <td><%= item.id %></td>
            <td><%= link_to item.name, admin_item_path(item.id) %></td>
            <td><%= item.price %></td>
            <td><%= item.genre.name %></td>
            <td><%= item.is_active %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    </div>
  </div>
</div>